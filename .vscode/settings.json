{
    "phpsab.snifferEnable" : false,
    "phpsab.autoRulesetSearch": false,
    "phpsab.executablePathCBF": "${pwd}/../vendor/squizlabs/php_codesniffer/bin/phpcbf",
    "phpsab.executablePathCS": "${pwd}/../vendor/squizlabs/php_codesniffer/bin/phpcs",
    "[php]": {
        "editor.defaultFormatter": "valeryanm.vscode-phpsab"
    },
    "extensions.ignoreRecommendations": false,
    "phpsab.snifferShowSources": true,
    "phpsab.standard": "NewFold",
    "phpsab.allowedAutoRulesets": [
        ".phpcs.xml",
        ".phpcs.xml.dist",
        "phpcs.xml",
        "phpcs.xml.dist",
        "phpcs.ruleset.xml",
        "ruleset.xml"
    ],
    "phpcs.standard": "NewFold",
    "files.autoSave": "off",
    "beautify.config": "",
    "cSpell.words": [
        "absint",
        "Amartya",
        "autoload",
        "autoloader",
        "dealerdirect",
        "Httplug",
        "newfold",
        "phpcbf",
        "phpcodesniffer",
        "phpcs",
        "TYPESENSE",
        "userdata"
    ],
}
